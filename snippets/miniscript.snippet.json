{
	"pk": {
			"prefix": ["pk"],
			"body": ["pk(${1:key})"],
			"description": "check(key)"
	},
	"pk_h": {
			"prefix": ["pk_h"],
			"body": ["pk_h(${1:key})"],
			"description": "check(key)"
	},
	"older": {
			"prefix": ["older"],
			"body": ["older(${1:n})"],
			"description": "nSequence ≥ n (and compatible)"
	},
	"after": {
			"prefix": ["after"],
			"body": ["after(${1:n})"],
			"description": "nLockTime ≥ n (and compatible)"
	},
	"sha256": {
			"prefix": ["sha256"],
			"body": ["sha256(${1:h})"],
			"description": "len(x) = 32 and SHA256(x) = h"
	},
	"hash256": {
			"prefix": ["hash256"],
			"body": ["hash256(${1:h})"],
			"description": "len(x) = 32 and HASH256(x) = h"
	},
